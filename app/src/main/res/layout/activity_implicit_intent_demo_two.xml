<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/llExplicitIntentDemoTwoMain"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical"
    android:padding="@dimen/dp_20"
    tools:context=".intent.ActivityImplicitIntentDemoTwo">


    <EditText
        android:id="@+id/edtUrl"
        style="@style/editTextBMI"
        android:autofillHints=""
        android:hint="@string/enter_valid_url"
        android:inputType="textUri"
        tools:ignore="DuplicateSpeakableTextCheck,TextContrastCheck,VisualLintTextFieldSize" />

    <Button
        android:id="@+id/btnGoto"
        style="@style/btnBMI"
        android:text="@string/go_to"
        android:textAllCaps="false"
        tools:ignore="TextContrastCheck" />
</LinearLayout>